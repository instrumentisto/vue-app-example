{
  "name": "vue-app-example",
  "scripts": {
    "build": "npm run build:server && npm run build:client",
    "build:client": "cross-env NODE_ENV=development webpack --config webpack/client.config.js --progress --hide-modules",
    "build:server": "cross-env NODE_ENV=development webpack --config webpack/server.config.js --progress --hide-modules",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "json-server": "json-server --watch .data/db.json",
    "test:unit": "karma start test/unit/karma.conf.js --single-run"
  },
  "dependencies": {
    "accept-language-parser": "^1.4.0",
