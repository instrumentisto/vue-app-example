{
  "name": "vue-app-example",
  "scripts": {
    "build": "npm run build:server && npm run build:client",
    "build:client": "cross-env NODE_ENV=development API_URL=http://vue-app-example.dev:3000 webpack --config webpack/client.config.js --progress --hide-modules",
    "build:server": "cross-env NODE_ENV=development API_URL=http://json-server:3000 webpack --config webpack/server.config.js --progress --hide-modules",
    "json-server": "json-server --watch .data/db.json --delay 500",
    "test": "npm run test:unit && npm run test:e2e",
    "test:e2e": "npm run build:client && tsc -p test/e2e && nightwatch --config test/e2e/nightwatch.conf.js --env chrome",
    "test:unit": "karma start test/unit/karma.conf.js --single-run"
  },
  "dependencies": {
    "accept-language-parser": "^1.4.0",
    "axios": "^0.16.1",
    "express": "^4.15.2",
    "node-localstorage": "^1.3.0",
    "serve-favicon": "^2.4.2",
    "vee-validate": "^2.0.0-beta.25",
    "vue": "^2.2.1",
    "vue-class-component": "5.0.1",
    "vue-i18n": "^6.0.0",
    "vue-router": "^2.3.1",
    "vue-server-renderer": "^2.2.6"
  },
  "devDependencies": {
    "@types/chai": "^3.5.0",
    "@types/mocha": "^2.2.40",
    "@types/node": "^7.0.13",
    "@types/vue-i18n": "^6.1.0",
    "@types/webpack-env": "^1.13.0",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "chai": "^3.5.0",
    "chromedriver": "^2.29.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^3.0.0",
    "cross-spawn": "^5.1.0",
    "css-loader": "^0.28.0",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "json-server": "^0.10.0",
    "karma": "^1.6.0",
    "karma-chai": "^0.1.0",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.4",
